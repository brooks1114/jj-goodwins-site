{
  "public": "build",
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/jj-goodwins-site/{,/**}",
      "destination": "/index.html"
    },
    {
      "source": "/jj-goodwins-site/static/**",
      "destination": "/static/$1"
    },
    {
      "source": "/jj-goodwins-site/assets/**",
      "destination": "/assets/$1"
    },
    {
      "source": "/jj-goodwins-site/*.{json,ico}",
      "destination": "/$1"
    }
  ]
}